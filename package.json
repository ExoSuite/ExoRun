{
  "name": "ExoRun",
  "version": "0.9.11",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean": "rm -rf $TMPDIR/react-*",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "test": "jest --maxWorkers=4",
    "test-ci": "jest --maxWorkers=4 --coverage --ci --reporters=default --reporters=jest-junit",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p . -e 'node_modules'",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint -p . -e 'node_modules'",
    "postinstall": "solidarity",
    "hack:patch-rctmgl": "cp ./hacks/react-native-mapbox-gl/build.gradle node_modules/@react-native-mapbox-gl/maps/android/rctmgl",
    "hack:patch-androidX": "jetify",
    "reload-android": "adb shell input text 'RR'",
    "reactotron-android": "adb reverse tcp:9090 tcp:9090",
    "packager-android": "adb reverse tcp:8081 tcp:8081",
    "version:patch": "npm version patch -m 'Upgrade to %s'",
    "prepare": "npm-run-all patch hack:*",
    "patch": "patch-package",
    "postversion": "react-native-version",
    "version:minor": "npm version minor -m 'Upgrade to %s'",
    "version:major": "npm version major -m 'Upgrade to %s'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-mapbox-gl/maps": "7.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/midpoint": "^5.1.5",
    "apisauce": "1.0.3",
    "autobind-decorator": "^2.4.0",
    "core-js": "^3",
    "emoji-utils": "^1.0.1",
    "i18n-js": "^3.3.0",
    "idx": "^2.5.6",
    "jwt-decode": "^2.2.0",
    "laravel-echo": "^1.5.4",
    "lodash-es": "^4.17.15",
    "lottie-ios": "3.0.3",
    "lottie-react-native": "^3.1.0",
    "mobx": "^5.13.0",
    "mobx-react": "^5.4.4",
    "mobx-state-tree": "^3.14.1",
    "moment": "^2.24.0",
    "node-libs-react-native": "^1.0.3",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-animatable": "^1.3.2",
    "react-native-config": "^0.11.7",
    "react-native-device-info": "^2.3.2",
    "react-native-fs": "^2.14.1",
    "react-native-gesture-handler": "1.3.0",
    "react-native-gifted-chat": "^0.9.11",
    "react-native-input-scroll-view": "^1.9.3",
    "react-native-keyboard-accessory": "^0.1.9",
    "react-native-keychain": "3.1.3",
    "react-native-localize": "^1.1.4",
    "react-native-modal": "^11.3.1",
    "react-native-modal-datetime-picker": "^7.5.0",
    "react-native-paper": "^2.16.0",
    "react-native-screens": "1.0.0-alpha.23",
    "react-native-sound": "^0.11.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-status-bar-height": "^2.4.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version-info": "^1.0.1",
    "react-native-video": "^5.0.0",
    "react-navigation": "3.11.1",
    "react-navigation-material-bottom-tabs": "^1.0.0",
    "react-viro": "^2.16.0",
    "reactotron-mst": "3.1.3",
    "reactotron-react-native": "^3.6.4",
    "shorthash": "^0.0.2",
    "socket.io-client": "^2.2.0",
    "tslib": "^1.10.0",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@fortawesome/fontawesome-common-types": "^0.2.21",
    "@storybook/addon-storyshots": "^5.1.10",
    "@storybook/react-native": "^5.1.10",
    "@storybook/theming": "^5.1.10",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.17",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.7.1",
    "@types/ramda": "0.26.19",
    "@types/react": "*",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/sinon": "^7.0.13",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "ignite-bowser": "^4.6.0",
    "ignite-vector-icons": "^1.1.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "jest-junit": "^7.0.0",
    "jest-preset-ignite": "0.6.1",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "0.55.0",
    "mock-socket": "^9.0.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.1.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.18.2",
    "prop-types": "^15.7.2",
    "react-devtools-core": "3.6.3",
    "react-dom": "16.8.6",
    "react-native-typescript-transformer": "1.2.13",
    "react-native-version": "^3.1.0",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "sinon": "^7.4.1",
    "sinon-chai": "3.3.0",
    "solidarity": "2.3.1",
    "tslint": "5.18.0",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "^0.5.4"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./test-reports",
    "outputName": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
