{
  "name": "ExoRun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "test": "jest",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p .",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint -p .",
    "postinstall": "solidarity",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "patch": "patch-package",
    "prepare": "npm-run-all patch hack:*",
    "storybook": "storybook start -p 9001 --skip-packager"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@types/node": "^10.12.5",
    "apisauce": "1.0.0",
    "autobind-decorator": "^2.2.1",
    "es6-symbol": "^3.1.1",
    "i18n-js": "^3.1.0",
    "jsc-android": "^236355.0.0",
    "jwt-decode": "^2.2.0",
    "lodash.throttle": "4.1.1",
    "mobx": "5.6.0",
    "mobx-react": "5.3.6",
    "mobx-state-tree": "3.7.1",
    "node-libs-react-native": "^1.0.3",
    "ramda": "0.25.0",
    "react": "16.6.1",
    "react-native": "0.57.4",
    "react-native-keychain": "3.0.0",
    "react-native-languages": "^3.0.1",
    "react-native-splash-screen": "3.1.1",
    "react-native-vector-icons": "6.0.2",
    "react-navigation": "2.18.2",
    "reactotron-mst": "2.1.0",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@storybook/addon-storyshots": "github:infinitered/addon-storyshots",
    "@storybook/react-native": "4.0.4",
    "@types/jest": "23.3.9",
    "@types/ramda": "0.26.0",
    "@types/react": "16.7.1",
    "@types/react-native": "0.57.7",
    "@types/react-navigation": "2.13.1",
    "@types/react-test-renderer": "16.0.3",
    "@types/validate.js": "0.11.0",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "ignite-ir-boilerplate-bowser": "^1.0.0",
    "ignite-vector-icons": "^1.1.0",
    "jest": "23.6.0",
    "jest-preset-ignite": "0.6.1",
    "metro-react-native-babel-preset": "0.49.0",
    "npm-run-all": "4.1.3",
    "patch-package": "5.1.1",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.15.1",
    "react-dom": "16.6.1",
    "react-native-typescript-transformer": "1.2.10",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.6.1",
    "reactotron-react-native": "^2.1.0",
    "rimraf": "2.6.2",
    "solidarity": "2.1.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.1.6"
  },
  "jest": {
    "preset": "jest-preset-ignite",
    "testPathIgnorePatterns": [
      "/node_modules"
    ],
    "globals": {
      "__TEST__": true
    }
  },
  "rnpm": {
    "assets": [
      "./src/theme/fonts/",
      "./src/assets"
    ]
  }
}
