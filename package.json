{
  "name": "ExoRun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn react-native start",
    "clean": "rm -rf $TMPDIR/react-*",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "test": "jest",
    "test-ci": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p . -e 'node_modules'",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint -p . -e 'node_modules'",
    "postinstall": "solidarity",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "hack:lottie-react-native": "./hacks/lottie-react-native-hack.sh",
    "reload-android": "adb shell input text 'RR'",
    "reactotron-android": "adb reverse tcp:9090 tcp:9090",
    "packager-android": "adb reverse tcp:8081 tcp:8081",
    "patch": "patch-package",
    "prepare": "npm-run-all patch hack:*",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.2.1",
    "apisauce": "1.0.2",
    "autobind-decorator": "^2.4.0",
    "es6-symbol": "^3.1.1",
    "i18n-js": "^3.2.1",
    "jsc-android": "^236355.1.1",
    "jwt-decode": "^2.2.0",
    "lodash.assign": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.includes": "^4.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.merge": "^4.6.1",
    "lodash.throttle": "4.1.1",
    "lottie-react-native": "^2.6.0",
    "mobx": "5.9.0",
    "mobx-react": "5.4.3",
    "mobx-state-tree": "3.12.0",
    "node-libs-react-native": "^1.0.3",
    "ramda": "0.26.1",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-animatable": "^1.3.2",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "1.1.0",
    "react-native-keyboard-accessory": "^0.1.9",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-keychain": "3.1.1",
    "react-native-localize": "^1.1.0",
    "react-native-modal": "^9.0.0",
    "react-native-sound": "^0.10.12",
    "react-native-splash-screen": "3.2.0",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "3.5.1",
    "react-viro": "^2.13.0",
    "reactotron-mst": "3.1.0",
    "reactotron-react-native": "^3.2.1",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/runtime": "^7.4.0",
    "@storybook/addon-storyshots": "^4.1.14",
    "@storybook/react-native": "^4.1.14",
    "@types/jest": "24.0.11",
    "@types/node": "^11.11.4",
    "@types/ramda": "0.26.3",
    "@types/react": "16.8.8",
    "@types/react-native": "0.57.41",
    "@types/react-navigation": "3.0.5",
    "@types/react-test-renderer": "16.8.1",
    "@types/validate.js": "0.11.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "ignite-bowser": "^4.0.0-alpha.3",
    "ignite-vector-icons": "^1.1.1",
    "jest": "24.5.0",
    "jest-junit": "^6.3.0",
    "jest-preset-ignite": "0.6.1",
    "metro-react-native-babel-preset": "0.53.1",
    "npm-run-all": "4.1.5",
    "patch-package": "6.0.5",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.16.4",
    "react-devtools-core": "3.6.1",
    "react-dom": "16.8.4",
    "react-native-typescript-transformer": "1.2.12",
    "react-native-version": "^2.7.0",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.8.4",
    "rimraf": "2.6.3",
    "solidarity": "2.3.1",
    "tslint": "5.14.0",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "tslint-react": "^3.6.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.3.4000"
  },
  "jest": {
    "preset": "jest-preset-ignite",
    "transformIgnorePatterns": [
      "/node_modules/@react-native-community/async-storage/(?!(lib))"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./test-reports",
    "outputName": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "resolutions": {
    "@types/react": "16.8.8"
  },
  "rnpm": {
    "assets": [
      "./app/theme/fonts/",
      "./app/assets"
    ]
  }
}
