{
  "name": "ExoRun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "rm -rf $TMPDIR/react-*",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "test": "jest",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p .",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint -p .",
    "postinstall": "solidarity",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "patch": "patch-package",
    "prepare": "npm-run-all patch hack:*",
    "storybook": "storybook start -p 9001 --skip-packager"
  },
  "dependencies": {
    "apisauce": "1.0.2",
    "autobind-decorator": "^2.4.0",
    "es6-symbol": "^3.1.1",
    "i18n-js": "^3.2.1",
    "jsc-android": "^236355.1.1",
    "jwt-decode": "^2.2.0",
    "lodash.includes": "^4.3.0",
    "lodash.throttle": "4.1.1",
    "lottie-react-native": "^2.5.11",
    "mobx": "5.9.0",
    "mobx-react": "5.4.3",
    "mobx-state-tree": "3.10.2",
    "node-libs-react-native": "^1.0.3",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-native": "0.58.3",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "1.0.15",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-keychain": "3.0.0",
    "react-native-languages": "^3.0.2",
    "react-native-modal": "^7.0.2",
    "react-native-splash-screen": "3.1.1",
    "react-native-vector-icons": "6.2.0",
    "react-navigation": "3.1.2",
    "reactotron-mst": "2.1.4",
    "reactotron-react-native": "^2.1.5",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "@storybook/addon-storyshots": "^3.4.11",
    "@storybook/react-native": "^3.4.11",
    "@types/jest": "23.3.13",
    "@types/node": "^10.12.21",
    "@types/ramda": "0.26.0",
    "@types/react": "16.8.1",
    "@types/react-native": "0.57.32",
    "@types/react-navigation": "3.0.1",
    "@types/react-test-renderer": "16.0.3",
    "@types/validate.js": "0.11.0",
    "babel-jest": "24.0.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "ignite-bowser": "^3.0.0",
    "ignite-vector-icons": "^1.1.1",
    "jest": "24.0.0",
    "jest-junit": "^6.2.1",
    "jest-preset-ignite": "0.6.1",
    "metro-react-native-babel-preset": "0.51.1",
    "npm-run-all": "4.1.5",
    "patch-package": "6.0.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.16.4",
    "react-devtools-core": "3.6.0",
    "react-dom": "16.7.0",
    "react-native-typescript-transformer": "1.2.11",
    "react-powerplug": "1.0.0",
    "react-test-renderer": "16.7.0",
    "rimraf": "2.6.3",
    "solidarity": "2.3.1",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.3.1"
  },
  "jest": {
    "preset": "jest-preset-ignite",
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "rnpm": {
    "assets": [
      "./app/theme/fonts/",
      "./app/assets"
    ]
  }
}
